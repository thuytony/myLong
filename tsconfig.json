{
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ],
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["es6"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": ".",
    "paths": {
      "@assets": ["src/assets"],
      "@components": ["src/components"],
      "@constants": ["src/constants"],
      "@module": ["src/module"],
      "@navigation": ["src/navigation"],
      "@res": ["src/res"],
      "@utils": ["src/utils"],
      "@redux": ["src/redux"],
      "@model": ["src/model"],
      "@networking": ["src/networking"],
    }
  },
}